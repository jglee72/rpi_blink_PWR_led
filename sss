+#ifndef __GTK_WIDGET_H__ define __GTK_WIDGET_H__ include <gdk/gdk.h> 
+#include <gtk/gtkaccelerator.h> include <gtk/gtkobject.h> include 
+#<gtk/gtkstyle.h> ifdef __cplusplus
+extern "C" {
