diff --git a/examples/blink/test.h b/examples/blink/test.h
new file mode 100644
index 0000000..e22c9b9
--- /dev/null
+++ b/examples/blink/test.h
@@ -0,0 +1,16 @@
+//test.h
+
+int kbhit(void);
+
+int kbhit()
+{
+    struct timeval tv;
+    fd_set fds;
+    tv.tv_sec = 0;
+    tv.tv_usec = 0;
+    FD_ZERO(&fds);
+    FD_SET(STDIN_FILENO, &fds); //STDIN_FILENO is 0
+    select(STDIN_FILENO+1, &fds, NULL, NULL, &tv);
+    return FD_ISSET(STDIN_FILENO, &fds);
+}
+
